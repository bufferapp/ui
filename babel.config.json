{
  "env": {
    "production": {
      "plugins": [
        [
          "babel-plugin-styled-components",
          {
            "pure": true,
            "namespace": "bufferapp-ui",
            "minify": true
          }
        ]
      ]
    }
  },
  "plugins": [
    "@babel/plugin-transform-modules-commonjs",
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-export-default-from"
  ],
  "presets": [
    "@babel/preset-env",
    "@babel/react"
  ]
}
